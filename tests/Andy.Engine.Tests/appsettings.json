{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DefaultProvider": "OpenAI",
  "Providers": {
    "OpenAI": {
      "ApiKey": "${OPENAI_API_KEY}",
      "BaseUrl": "${OPENAI_BASE_URL}",
      "Model": "${OPENAI_MODEL}",
      "Priority": 0
    }
  }
}
