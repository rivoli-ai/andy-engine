{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Llm": {
    "DefaultProvider": "OpenAI",
    "Providers": {
      "OpenAI": {
        "ApiKey": "your-openai-api-key-here",
        "ApiBase": "https://api.openai.com/v1",
        "Model": "gpt-4o",
        "Enabled": true
      },
      "Cerebras": {
        "Enabled": false
      },
      "Ollama": {
        "Enabled": false
      },
      "Anthropic": {
        "Enabled": false
      }
    }
  }
}
